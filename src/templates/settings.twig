{% import "_includes/forms" as forms %}

{% do view.registerAssetBundle("julianmjones\\schedapiintegration\\assetbundles\\schedapiintegration\\SchedApiIntegrationAsset") %}

{{ forms.textField({
    label: 'Sched API Key',
    instructions: 'Enter your Sched Api Key here',
    id: 'schedApiKey',
    name: 'schedApiKey',
    value: settings['schedApiKey']})
}}

{{ forms.textField({
    label: 'Sched Conference ID',
    instructions: 'Enter your Sched Conference ID here',
    id: 'conferenceId',
    name: 'conferenceId',
    value: settings['conferenceId']})
}}